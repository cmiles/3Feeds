<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ThreeFeeds.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="using:ThreeFeeds.Pages"
    Title="3Feeds"
    FlyoutBehavior="Flyout">
    <ShellContent
        Title="Feed"
        ContentTemplate="{DataTemplate pages:FeedItemListPage}"
        Route="Feed" />
    <ShellContent
        Title="Settings"
        ContentTemplate="{DataTemplate pages:SettingsPage}"
        Route="Settings" />
    <ShellContent
        Title="Help/About"
        ContentTemplate="{DataTemplate pages:HelpAboutPage}"
        Route="HelpAbout" />
</Shell>
